{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "prepare": "husky",
    "start": "ts-node src/index.ts",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:migration:generate": "npm run typeorm -- -d ./src/config/data-source.ts migration:generate",
    "typeorm:migration:revert": "npm run typeorm -- -d ./src/config/data-source.ts migration:revert",
    "typeorm:migration:run": "npm run typeorm -- -d ./src/config/data-source.ts migration:run"
  },
  "prettier": "@vercel/style-guide/prettier",
  "dependencies": {
    "@automapper/classes": "8.8.1",
    "@automapper/core": "8.8.1",
    "@automapper/nestjs": "8.8.1",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/swagger": "7.4.0",
    "@nestjs/throttler": "6.0.0",
    "@nestjs/typeorm": "10.0.2",
    "@nestjsx/crud": "4.6.2",
    "@nestjsx/crud-request": "4.6.2",
    "@nestjsx/crud-typeorm": "4.6.2",
    "bcrypt": "5.1.1",
    "cache-manager": "5.7.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dayjs": "1.11.12",
    "dotenv": "16.4.5",
    "env-var": "7.5.0",
    "helmet": "7.1.0",
    "lodash": "4.17.21",
    "passport-jwt": "4.0.1",
    "pg": "8.12.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.3",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.3",
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/lodash": "4.17.7",
    "@types/ms": "0.7.34",
    "@types/node": "22.0.0",
    "@types/passport-jwt": "4.0.1",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vercel/style-guide": "6.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.3",
    "prettier": "3.3.3",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
